<h1>New Post</h1>
<%= form_for :post, url:{action: :create} do |f|%>
<p><%=f.label :title%></p>
<p><%=f.text_field :title%></p>
<p><%=f.label :text%></p>
<p><%=f.text_area :text%></p>
<p><%=f.submit :disable_with=>'processing', :onClick=>"check();"%></p>
<%end%>

<script>
	function check()
	{
		alert("do u want to save??")
	}
</script>
